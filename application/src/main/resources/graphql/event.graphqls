type Event {
    id: String!
    memberOne: Member!
    memberTwo: Member!
    dateTime: LocalDateTime!
}

type Member {
    competitorId: String!
    username: String!
    deviation: Int!
    rating: Int!
}

input AddEventInput {
    leagueId: String!
    memberOneId: String!
    memberTwoId: String!
    dateTime: LocalDateTime!
}
